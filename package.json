{
  "name": "yashvardhan'schatapp",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "turbo start",
    "db:generate": "cd ./packages/db && npx prisma generate",
    "db:migrate": "cd ./packages/db && npx prisma migrate deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-backend": "cd ./apps/backend && node dist/index.js",
    "start-frontend": "cd ./apps/chat-app && npm start"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.9",
    "typescript": "^5.4.5",
    "concurrently": "^7.2.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "yarn": "^1.22.22"
  }
}
